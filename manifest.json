{
    "name": "Wallabag",
    "id": "wallabag",
    "description": {
        "en": "a self hostable read-it-later app"
    },
    "developer": {
        "name": "beudbeud",
        "email": "beudbeud@beudibox.fr",
        "url": "http://www.inthewallabag.com/"
    },
    "multi_instance": "true",
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "ask": {
                    "en": "Choose a domain for Wallabag"
                },
                "example": "domain.org"
            },
            {
                "name": "path",
                "ask": {
                    "en": "Choose a path for Wallabag"
                },
                "example": "/wallabag",
                "default": "/wallabag"
            }
        ]
    }
}


